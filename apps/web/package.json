{
  "name": "@malang-dev/web",
  "version": "1.0.0",
  "description": "Vue.js web application",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.{ts,vue}\" --fix",
    "test": "vitest --passWithNoTests",
    "test:cov": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-vue-next": "^0.575.0",
    "pinia": "^3.0.0",
    "radix-vue": "^1.9.0",
    "tailwind-merge": "^2.5.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@malang-dev/eslint-config": "workspace:*",
    "@malang-dev/typescript-config": "workspace:*",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^9.39.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^20.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.4.2",
    "vite": "^5.4.0",
    "vitest": "^2.0.0",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": ">= 20"
  }
}
